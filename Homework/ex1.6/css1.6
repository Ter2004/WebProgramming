.banner {
  background-color: rgb(194, 255, 242);
  margin: 0;
  width: 8rem;
  height: 4rem;
  text-align: center;
  line-height: 4rem;
  position: absolute;
  right: 50%;
  transform: translate(50%,0);
  border: 2px solid black;
  top: -0.5em;
}

.container{
  position: relative;
  margin: 1em;
  height: 25em;
  background-color: rgb(11, 197, 11);
}

.close{
  position: absolute;
  right: 0;
}
.text{
  color: white;
  font-size: xx-large;
  position: absolute;
  text-align: center;
  right: 50%;
  bottom: 50%;
  transform: translate(50%,0);
  border: 1px dashed white;
  padding: 0.5%;
}
.left{
  border-radius: 100%;
  width: 10em;
  height: 10em;
  background-color: white;
  position: absolute;
  bottom: 50%;
  left: 0.5%;
  transform: translate(-50%, 50%);
}
.bottom{
  border-radius: 80px;
  width: 30em;
  height: 30em;
  background-color: white;
  position: absolute;
  bottom: -40%;
  left: 110%;
  transform: translate(-50%, 50%);
}
